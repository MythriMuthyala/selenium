pipeline {
      agent any 
      stages {
           stage('Checkout Code') {
              steps {
                  git 'https://github.com/your-repo/selenium-tests.git'
             }
         }
stage('Install Dependancies') {
    steps{
       bat 'pip install -r requirements.txt'
      }
  }
  stage('Run Selenium Tests') {
      steps {
           bat 'python seleniu_test.py'
       }    
   } 
 }